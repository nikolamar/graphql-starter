{
  "name": "graphql-starter-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dist": "nodemon dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "gen-dat": "npx typeorm migration:create -n gen-data",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "gen-mig": "npx typeorm migration:generate -n tables",
    "deploy": "./scripts/deploy.sh",
    "lint": "node_modules/.bin/tslint -c ./tslint.json './src/**/*.ts'",
    "lint-fix": "node_modules/.bin/tslint --fix -c ./tslint.json './src/**/*.ts'"
  },
  "author": "",
  "license": "ISC",
  "useTsNode": true,
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.17.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.16",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/sharp": "^0.27.1",
    "@types/uuid": "^8.3.0",
    "gen-env-types": "^1.0.5",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.19.1",
    "argon2": "^0.27.1",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "sharp": "^0.27.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  }
}
