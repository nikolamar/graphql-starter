{
  "name": "graphql-starter-nextjs-apollo",
  "version": "1.0.0",
  "author": "nikolatec",
  "keywords": [
    "graphql",
    "starter",
    "nextjs",
    "apollo"
  ],
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:env:types": "gen-env-types .env.local -o src/env.d.ts -e .",
    "tslint": "node_modules/.bin/tslint -c ./tslint.json './src/**/*.ts'",
    "tslint:fix": "node_modules/.bin/tslint --fix -c ./tslint.json './src/**/*.ts'",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@chakra-ui/icons": "^1.0.4",
    "@chakra-ui/react": "^1.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "apollo-upload-client": "^14.1.3",
    "formik": "^2.2.6",
    "framer-motion": "^2.9.4",
    "graphql": "^15.5.0",
    "moment": "^2.29.1",
    "next": "^10.0.2",
    "next-apollo": "^5.0.4",
    "next-pwa": "^3.1.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@netlify/plugin-nextjs": "^1.0.3",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "gen-env-types": "^1.0.5",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
